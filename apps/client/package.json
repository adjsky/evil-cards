    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "eslint-config-next": "13.0.2",
    "jsdom": "^22.1.0",
    "mock-socket": "^9.2.1",
    "next-sitemap": "^3.1.49",
    "npm-run-all": "^4.1.5",
