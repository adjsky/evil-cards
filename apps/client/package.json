{
  "name": "@evil-cards/client",
  "version": "0.7.2",
  "private": true,
  "scripts": {
    "ts-check": "tsc --noEmit",
    "build": "next build",
    "build-with-sitemap": "run-s build generate-sitemap",
    "build:production": "cross-env NEXT_PUBLIC_SITE_HOST=https://evilcards.online NEXT_PUBLIC_BALANCER_PATH=/api/server WITH_PWA=true NEXT_PUBLIC_WITH_ANALYTICS=true NEXT_PUBLIC_WITH_SENTRY=true npm run build-with-sitemap",
    "build:stage": "cross-env NEXT_PUBLIC_SITE_HOST=https://stage.evilcards.online NEXT_PUBLIC_BALANCER_PATH=/api/server WITH_PWA=true npm run build-with-sitemap",
    "dev": "next dev",
    "lint": "next lint",
    "start": "cross-env SKIP_CLIENT_ENV_VALIDATION=true next start",
    "analyze": "cross-env ANALYZE=true next build",
    "generate-sitemap": "next-sitemap --config next-sitemap.config.mjs"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^1.3.0",
    "@formkit/auto-animate": "1.0.0-beta.6",
