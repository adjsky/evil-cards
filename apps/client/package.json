    "clsx": "^1.2.1",
    "easy-speech": "^2.0.0",
    "interweave": "^13.0.0",
    "jotai": "^1.9.1",
    "ky": "^0.33.3",
    "neverthrow": "^6.0.0",
    "next": "^13.2.4",
    "next-plausible": "^3.6.4",
    "next-pwa": "^5.6.0",
    "ramda": "^0.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^1.12.0",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@evil-cards/server": "workspace:^",
    "@next/bundle-analyzer": "^13.0.2",
    "@next/env": "^13.1.6",
    "@svgr/webpack": "^8.0.1",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/easy-speech": "^1.2.0",
